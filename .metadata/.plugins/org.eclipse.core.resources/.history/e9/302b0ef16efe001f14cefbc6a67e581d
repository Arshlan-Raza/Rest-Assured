package day3;

import org.testng.annotations.*;

import static io.restassured.RestAssured.*;

public class CookieDemo {
	
	@Test
	void testCookie() {
		given() 
		
		.when()
			.get("https://www.google.com")
		
		.then()
			.cookie("AEC","AVcja2e4Pmbi-Mws0XIjVOeJWVHUGwTHXvVGBWZj3utkeMwOcZZs2oVkUzE")
			.log().all();

	}

}
